{
    "manifest_version": 3,
    "name": "MSN Dislike Viewer",
    "version": "1.0.1",
    "description": "Displays the dislike count on MSN articles.",
    "content_scripts": [{
        "matches": ["https://*.msn.com/*"],
        "js": ["content.js"]
    }],
    "icons": {
        "16": "msn_16.png",
        "32": "msn_32.png",
        "48": "msn_48.png",
        "128": "msn_128.png"
    }
}
